@model ContatoModel
@{
    ViewData["Title"] = "Confirmação de Exclusão";

}

<div class="text-center">
    <h3>Realmente deseja apagar esse Contato @Model.Nome</h3>
</div>
<a role="button" class="btn btn-danger  btn-block" asp-controller="Contato" asp-route-id="@Model.Id"  asp-action="ConfirmacaoApagarContato">Confirmar Exclusão</a>
<a role="button" class="btn btn-primary  btn-block" asp-controller="Contato" asp-action="Index">Cancelar Exclusão</a>

